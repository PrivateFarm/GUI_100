-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Heading = Instance.new("TextLabel")
local Speed = Instance.new("TextButton")
local SmallSize = Instance.new("TextButton")
local _1Shot = Instance.new("TextButton")
local FastPunch = Instance.new("TextButton")
local Loop = Instance.new("TextButton")
local TextButton = Instance.new("TextButton")
local Credits = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.Position = UDim2.new(0.64269489, 0, 0.530017138, 0)
Frame.Size = UDim2.new(0, 440, 0, 241)
Frame.Active = true
Frame.Draggable = true

Heading.Name = "Heading"
Heading.Parent = Frame
Heading.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Heading.Size = UDim2.new(0, 440, 0, 50)
Heading.Font = Enum.Font.Creepster
Heading.Text = "Boxing Sim 2 HUB v2"
Heading.TextColor3 = Color3.fromRGB(181, 181, 181)
Heading.TextSize = 32.000
Heading.TextWrapped = true

Speed.Name = "Speed"
Speed.Parent = Frame
Speed.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Speed.Position = UDim2.new(0.0424242429, 0, 0.269709557, 0)
Speed.Size = UDim2.new(0, 116, 0, 50)
Speed.Font = Enum.Font.Creepster
Speed.Text = "Speed X"
Speed.TextColor3 = Color3.fromRGB(181, 181, 181)
Speed.TextSize = 28.000
Speed.MouseButton1Down:connect(function()
	local walkspeedplayer = game:GetService("Players").LocalPlayer
	local walkspeedmouse = walkspeedplayer:GetMouse()

	local walkspeedenabled = false

	function x_walkspeed(key)
		if (key == "x") then
			if walkspeedenabled == false then
				_G.WS = 5000;
				local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
				Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
					Humanoid.WalkSpeed = _G.WS;
				end)
				Humanoid.WalkSpeed = _G.WS;

				walkspeedenabled = true
			elseif walkspeedenabled == true then
				_G.WS = 300;
				local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
				Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
					Humanoid.WalkSpeed = _G.WS;
				end)
				Humanoid.WalkSpeed = _G.WS;

				walkspeedenabled = false
			end
		end
	end

	walkspeedmouse.KeyDown:connect(x_walkspeed)

end)

SmallSize.Name = "Small Size"
SmallSize.Parent = Frame
SmallSize.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
SmallSize.Position = UDim2.new(0.366161644, 0, 0.269709557, 0)
SmallSize.Size = UDim2.new(0, 116, 0, 50)
SmallSize.Font = Enum.Font.Creepster
SmallSize.Text = "Small"
SmallSize.TextColor3 = Color3.fromRGB(181, 181, 181)
SmallSize.TextSize = 30.000
SmallSize.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetChildren()) do
		if v:IsA("NumberValue") then
			v:Destroy()
		end
	end
end)

