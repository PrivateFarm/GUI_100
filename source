end)

FastPunch.Name = "Fast Punch"
FastPunch.Parent = Frame
FastPunch.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
FastPunch.Position = UDim2.new(0.0409090891, 0, 0.556016624, 0)
FastPunch.Size = UDim2.new(0, 116, 0, 50)
FastPunch.Font = Enum.Font.Creepster
FastPunch.Text = "Punch Faster"
FastPunch.TextColor3 = Color3.fromRGB(181, 181, 181)
FastPunch.TextSize = 24.000
FastPunch.MouseButton1Click:Connect(function()
	old = hookfunction(wait,function(t)
		if not checkcaller() and getcallingscript().Name == "MouseIcon" then
			return old()
		end
		return old(t)
	end)
end)

